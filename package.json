{
  "name": "karet-examples",
  "private": true,
  "description": "Karet Examples",
  "main": "docs/app.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "lessc src/main.less docs/bundle.css",
    "build:js": "cp node_modules/babel-polyfill/dist/polyfill.min.js docs/app.js && NODE_ENV=production browserify src/app.js -t babelify -t envify -g uglifyify >> docs/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "npm run watch:js & npm run watch:css",
    "watch:css": "npm run build:css && watch-run -p 'src/*.less' 'npm run build:css'",
    "watch:js": "watchify src/app.js -v --loadMaps --debug -t babelify -o docs/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/karet-examples.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/karet-examples/issues"
  },
  "homepage": "https://github.com/calmm-js/karet-examples#readme",
  "dependencies": {
    "atom.storage": "^0.3.4",
    "atom.undo": "^0.3.2",
    "karet": "^0.1.3",
    "karet.util": "^0.2.1",
    "kefir": "^3.6.0",
    "kefir.atom": "^3.0.1",
    "partial.lenses": "^3.9.0",
    "ramda": "^0.22.1",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "envify": "^3.4.1",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "less": "^2.7.1",
    "uglifyify": "^3.0.4",
    "watch-run": "^1.2.4",
    "watchify": "^3.7.0"
  }
}
