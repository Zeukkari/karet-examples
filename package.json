{
  "name": "kral-examples",
  "private": true,
  "description": "Kefir+React+Atom Examples",
  "main": "public/app.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run polyfill",
    "build:css": "lessc src/main.less public/bundle.css",
    "build:js": "export NODE_ENV=production && browserify src/app.js -t babelify -t envify -g uglifyify > public/app.js",
    "lint": "eslint src test",
    "polyfill": "cp node_modules/babel-polyfill/dist/polyfill.min.js public/polyfill.min.js",
    "prepublish": "npm run lint && npm run build",
    "watch": "npm run watch:js & npm run watch:css",
    "watch:css": "npm run build:css && watch-run -p 'src/*.less' 'npm run build:css'",
    "watch:js": "watchify src/app.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/kral-examples.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/kral-examples/issues"
  },
  "homepage": "https://github.com/calmm-js/kral-examples#readme",
  "dependencies": {
    "atom.storage": "^0.3.3",
    "atom.undo": "^0.3.1",
    "kefir": "^3.2.5",
    "kefir.atom": "^2.2.2",
    "kefir.react.html": "^3.2.4",
    "partial.lenses": "^3.7.0",
    "ramda": "^0.21.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.1",
    "eslint": "^3.0.0",
    "eslint-plugin-react": "^5.2.2",
    "less": "^2.7.1",
    "uglifyify": "^3.0.2",
    "watch-run": "^1.2.4",
    "watchify": "^3.7.0"
  }
}
